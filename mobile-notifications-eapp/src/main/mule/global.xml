  <secure-properties:config file="${env}-secure-properties.yaml" key="${encrypt.key}" name="secureEnvPropsConfig" doc:name="Secure Env Config Props">
    <secure-properties:encrypt algorithm="Blowfish"/>
  </secure-properties:config>
  <configuration-properties file="properties.yaml" doc:name="Config Props"/>
  <http:listener-config name="apiHttpListenerConfig">
    <http:listener-connection host="0.0.0.0" protocol="HTTP" port="${http.port}"/>
  </http:listener-config>
  <global-property name="encrypt.key" value="secure12345"/>
  <anypoint-mq:config name="amqConfig">
    <anypoint-mq:connection clientId="924f0aa4f18345fb8e02595f2461106a" clientSecret="9f571d2f80B94F2A890773E2585532B6" url="https://mq-ap-northeast-1.anypoint.mulesoft.com/api/v1/organizations/22832e58-de75-40d3-858b-d4d6bc5a489b/environments/6376d3a8-25a1-46ac-b1e8-3976d1aa2de7" />
</anypoint-mq:config>
<vm:config name="mobileAppNetworkVMConfig">
